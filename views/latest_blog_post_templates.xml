<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Khai bao custom resource-->
    <template id="latest_blog_post_snippet_assets_frontend" inherit_id="website.assets_frontend" name="Latest Blog Post Snippet Assets Frontend">
        <!--Khai bao file javascript-->
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/latest_blog_post_snippet/static/src/js/latest_blog_post_snippet.js"/>
        </xpath>
        <!--Khai bao file css-->
        <xpath expr="//link[last()]" position="after">
            <link rel='stylesheet' type="text/css" href='/latest_blog_post_snippet/static/src/css/style.css'/>
        </xpath>
    </template>
</odoo>